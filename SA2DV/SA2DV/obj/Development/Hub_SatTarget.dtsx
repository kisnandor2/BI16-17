    {
        base.PreExecute();
        H_Target_SQN = 0;
        SqlConnection conn = (SqlConnection)Connections.adonet.AcquireConnection(null);
        SqlCommand cmd = new SqlCommand();
        cmd.CommandText = "SELECT MAX(H_Target_SQN) FROM HubTarget";
        cmd.CommandType = CommandType.Text;
        cmd.Connection = conn;

        SqlDataReader reader;
        reader = cmd.ExecuteReader();

        while (reader.Read())
        {
            try
            {
                H_Target_SQN = reader.GetInt32(0); //get max H_Target_SQN
